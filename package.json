{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "browser": {
    "[module-name]": false
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "build": "yarn run clean-dist && yarn run compile && ./node_modules/.bin/webpack --config=configs/webpack/prod.js",
    "dev-build": "yarn run compile && ./node_modules/.bin/webpack --config=configs/webpack/dev.js",
    "clean-dist": "rimraf dist/*",
    "lint": "./node_modules/.bin/eslint src",
    "start": "yarn run compile && yarn run start-dev",
    "start-dev": "./node_modules/.bin/webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "./node_modules/.bin/jest --coverage --watchAll",
    "add-locale": "./node_modules/.bin/lingui add-locale",
    "extract": "./node_modules/.bin/lingui extract",
    "compile": "./node_modules/.bin/lingui compile",
    "translate": "./node_modules/.bin/lingui extract && ./node_modules/.bin/lingui compile",
    "analize-bundle": "yarn run clean-dist && yarn run compile && ./node_modules/.bin/webpack --config=configs/webpack/bundle-analysis.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^3.14.0",
    "@lingui/loader": "^3.14.0",
    "@lingui/macro": "^3.14.0",
    "@mui/icons-material": "^5.8.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.9",
    "@types/jest": "28.1.7",
    "@types/lingui__core": "^3.0.0",
    "@types/lingui__macro": "^3.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cache-loader": "^4.1.0",
    "css-loader": "^6.7.1",
    "eslint": "8.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "8.1.0",
    "jest": "^28.1.3",
    "mini-css-extract-plugin": "^2.6.1",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@lingui/react": "^3.14.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "axios": "^1.0.0-alpha.1",
    "camelcase": "^7.0.0",
    "camelcase-keys": "^8.0.1",
    "date-fns": "^2.29.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snakecase-keys": "^5.4.2",
    "tss-react": "^3.7.1"
  }
}
